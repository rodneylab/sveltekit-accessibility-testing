{
  "name": "sveltekit-accessibility-testing",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev --port 3000",
    "build": "npm run generate:manifest && svelte-kit build",
    "preview": "svelte-kit preview --port 3000",
    "check": "svelte-check --fail-on-hints",
    "check:watch": "svelte-check --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "lint:scss": "stylelint \"src/**/*.{css,scss,svelte}\"",
    "format": "prettier --write --plugin-search-dir=. .",
    "generate:images": "node ./generate-responsive-image-data.js",
    "generate:manifest": "node ./generate-manifest.js",
    "generate:sitemap": "node ./generate-sitemap.js",
    "prettier:check": "prettier --check --plugin-search-dir=. .",
    "prepare": "husky install",
    "test:a11y": "pa11y-ci --sitemap 'http://localhost:3000/sitemap.xml' --sitemap-find 'https://example.com' --sitemap-replace 'http://localhost:3000'",
    "test": "playwright test"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.1",
    "@fontsource/lato": "^4.5.3",
    "@fontsource/lora": "^4.5.4",
    "@fontsource/source-sans-pro": "^4.5.4",
    "@playwright/test": "^1.20.0",
    "@rodneylab/sveltekit-components": "^0.2.11",
    "@sveltejs/adapter-static": "^1.0.0-next.29",
    "@sveltejs/kit": "^1.0.0-next.301",
    "@types/object-hash": "^2.2.1",
    "commitlint": "^16.2.4",
    "dayjs": "^1.11.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "front-matter": "^4.0.2",
    "husky": "^7.0.4",
    "mdsvex": "^0.10.5",
    "object-hash": "^3.0.0",
    "pa11y": "^6.1.1",
    "pa11y-ci": "^3.0.1",
    "postcss": "^8.4.12",
    "postcss-html": "^1.3.0",
    "prettier": "~2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "reading-time": "1.4.0",
    "sass": "^1.49.9",
    "sharp": "^0.29.3",
    "stylelint": "^14.6.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-scss": "^4.2.0",
    "svelte": "^3.46.4",
    "svelte-check": "^2.4.6",
    "svelte-feather-icons": "^3.5.1",
    "svelte-preprocess": "^4.10.4",
    "svelte-preprocess-import-assets": "^0.1.6",
    "vanilla-lazyload": "^17.6.1",
    "vite-imagetools": "^4.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rodneylab/sveltekit-blog-mdx"
  },
  "bugs": {
    "url": "https://github.com/rodneylab/sveltekit-blog-mdx/issues"
  },
  "type": "module",
  "dependencies": {
    "s": "^1.0.0",
    "spnpm": "^1.0.0",
    "uninstall": "^0.0.0"
  }
}
